<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="GobezTalenTrade.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:GobezTalenTrade.Views"
	xmlns:local="clr-namespace:GobezTalenTrade">

	<TabBar>
		<ShellContent
			Title="Signin"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:SigninPage}" />
		<ShellContent
			Title="Home"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:HomePage}" />
		<ShellContent
			Title="PostSkill"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:PostSkillPage}" />
		<ShellContent
			Title="Settings"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:SettingsPage}" />
	</TabBar>
</Shell>
